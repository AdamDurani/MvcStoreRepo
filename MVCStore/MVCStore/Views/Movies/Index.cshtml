
@model MVCStore.Models.Movie[]

<h1>Movies</h1>

<table class="table">
    <tr>
        <th>Title</th>
        <th>Genre</th>
        <th>Release Date</th>
        <th>Price</th>
    </tr>
    
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Title</td>
            <td>@movie.Genre</td>
            <td>@movie.DateOfRelease.ToShortDateString()</td>
            <td>@movie.Price</td>
        </tr>
    }

</table>

